<div class="container">
  <div class="row">
    <div class="col-xs-12"></div>
    <h1>Add your dog</h1>
      <%= simple_form_for(@dog) do |f| %>
      <%= f.input :name %>
      <%= f.input :breed %>
      <%= f.input :age %>
      <%= f.input :description %>
      <%= f.input :size, collection: [ "Small", "Medium", "Large"]%>
      <%= f.input :location %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit %>
      <% end %>
  </div>
</div>




